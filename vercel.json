{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=43200"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "headers": {
        "Cache-Control": "s-maxage=86400, stale-while-revalidate=43200"
      }
    }
  ]
}
